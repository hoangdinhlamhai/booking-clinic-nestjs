# Supabase Configuration
# Copy file này thành .env và thay đổi các giá trị theo cấu hình Supabase của bạn

# Supabase URL và Keys (lấy từ Supabase Dashboard -> Settings -> API)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# ======================
# JWT Configuration
# ======================
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=604800

# ======================
# Payment
# ======================
SEPAY_API_KEY=

# Application
NODE_ENV=development
PORT=3000

